<%- model_class = OwnerRequest -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @owner_request.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:restaurant_id) %>:</strong></dt>
  <dd><%= @owner_request.restaurant_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:restaurant_name) %>:</strong></dt>
  <dd><%= @owner_request.restaurant_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:granted) %>:</strong></dt>
  <dd><%= @owner_request.granted %></dd>
  <dt><strong><%= model_class.human_attribute_name(:granted_by) %>:</strong></dt>
  <dd><%= @owner_request.granted_by %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              owner_requests_path, :class => 'btn'  %>
</div>
